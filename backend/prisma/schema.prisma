generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// USUARIO
model Usuario {
  id       Int    @id @default(autoincrement())
  nome     String
  email    String
  telefone String
  usuario  String @unique
  senha    String

  @@map("usuario")
}
